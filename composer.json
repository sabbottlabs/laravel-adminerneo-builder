yes{
    "name": "sabbottlabs/laravel-adminerneo-builder",
    "description": "Build tools for Laravel AdminerNeo package",
    "type": "library",
    "require": {
        "php": "^8.1",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "SabbottLabs\\AdminerNeoBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SabbottLabs\\AdminerNeoBuilder\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}